                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key28"
                style="@style/part_keyword"
                android:text="车" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key29"
                style="@style/part_keyword"
                android:text="轮" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key30"
                style="@style/part_keyword"
                android:text="轴" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key31"
                style="@style/part_keyword"
                android:text="转" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key32"
                style="@style/part_keyword"
                android:text="器" />
        </TableRow>

        <View
            android:layout_width="fill_parent"
            android:layout_height="0.01dp"
            android:background="@color/black" />

        <TableRow android:gravity="center_horizontal" >

            <Button
                android:id="@+id/lj_key33"
                style="@style/part_keyword"
                android:text="带" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key34"
                style="@style/part_keyword"
                android:text="齿" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key35"
                style="@style/part_keyword"
                android:text="梁" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key36"
                style="@style/part_keyword"
                android:text="架" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key37"
                style="@style/part_keyword"
                android:text="震" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key38"
                style="@style/part_keyword"
                android:text="簧" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key39"
                style="@style/part_keyword"
                android:text="臂" />

            <View
                android:layout_width="0.01dp"
                android:layout_height="fill_parent"
                android:background="@color/black" />

            <Button
                android:id="@+id/lj_key40"
                style="@style/part_keyword"
                android:text="杆" />
        </TableRow>
         
    </TableLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        android:visibility="gone" />

    <Button
        android:id="@+id/lj_cancle"
        android:layout_width="120dip"
        android:layout_height="40dp"
        android:layout_gravity="center_horizontal"
        android:background="@color/button_color"
        android:text="取消"
        android:visibility="gone" />

</LinearLayout>